{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.3" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" }
  ],
  "annotations": { "list": [ { "builtIn": 1, "datasource": { "type": "grafana", "uid": "-- Grafana --" }, "enable": true, "hide": true, "iconColor": "rgba(0, 211, 255, 1)", "name": "Annotations & Alerts", "target": { "limit": 100, "matchAny": false, "tags": [], "type": "dashboard" }, "type": "dashboard" } ] },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": 1,
  "links": [],
  "panels": [
    {
      "id": 1,
      "title": "ASP.NET Requests/s",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": "prometheus-ds-uid",
      "targets": [ { "expr": "sum(rate(http_server_request_duration_count[5m]))", "refId": "A" } ]
    },
    {
      "id": 2,
      "title": "ASP.NET Request Duration P95",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": "prometheus-ds-uid",
      "targets": [ { "expr": "histogram_quantile(0.95, sum(rate(http_server_request_duration_bucket[5m])) by (le))", "refId": "A" } ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "id": 3,
      "title": "GC Collections",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": "prometheus-ds-uid",
      "targets": [ { "expr": "sum(rate(process_runtime_dotnet_gc_collections_count[5m])) by (generation)", "legendFormat": "Gen {{generation}}", "refId": "A" } ]
    },
    {
      "id": 4,
      "title": "Exceptions",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": "prometheus-ds-uid",
      "targets": [ { "expr": "sum(rate(process_runtime_dotnet_exceptions_count[5m])) by (exception_type)", "legendFormat": "{{exception_type}}", "refId": "A" } ]
    }
  ],
  "refresh": "5s",
  "schemaVersion": 36,
  "style": "dark",
  "tags": [],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
