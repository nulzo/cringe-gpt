{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Authorization": "Information",
      "OllamaWebuiBackend": "Debug",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "DefaultOllamaApiUrl": "http://localhost:11434",
  "Streaming": {
    "TargetChunkSize": 3,
    "TargetIntervalMs": 3,
    "MaxBufferSize": 30,
    "EnableSmartBreaking": false,
    "EnableAdaptiveTiming": false,
    "MinChunkSize": 1,
    "ChunkSizeVariance": 0,
    "EnableVariableChunkSize": false,
    "FirstChunkDelayMs": 0,
    "EndOfStreamSpeedupFactor": 1.0
  }
}
